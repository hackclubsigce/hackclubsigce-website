{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  },
  "appAssociation": "AUTO",
  "rewrites": [
    {
      "source": "/home/**",
      "dynamicLinks": true
    }
  ]
}